<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
  "-//Checkstyle//DTD SuppressionFilter Configuration 1.0//EN"
  "https://checkstyle.org/dtds/suppressions_1_0.dtd">

<suppressions>
  <!-- Copyright in the license file is sufficient. The copyright header is not required in every source file -->
